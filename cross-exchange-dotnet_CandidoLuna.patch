+
+            TradeModel trade = new TradeModel()
+            {
+                Action = "SELL",
+                NoOfShares = 5,
+                Symbol = "XXX",
+                PortfolioId = 1
+            };
+
+            // Act
+            var result = await _tradeController.Post(trade) as BadRequestObjectResult;
+
+            // Assert
+            Assert.NotNull(result);
+            Assert.AreEqual((int)HttpStatusCode.BadRequest, result.StatusCode);
+        }
